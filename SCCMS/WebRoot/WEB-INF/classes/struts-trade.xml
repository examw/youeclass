<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
 <struts>
 	<package name="trade" namespace="/trade" 
 			extends="sccms-default">
 			<!-- 定义寄送管理模块的Action -->
 		<action name="tradeList" class="com.changh.sccms.action.trade.ListAction">
 			<result name="success" type = "json">
 				<param name="root">tradeList</param>
 			</result>
 		</action>
 		<action name="searchList" class="com.changh.sccms.action.trade.SearchAction">
 			<result name="success" type = "json">
 				<param name="root">tradeList</param>
 			</result>
 		</action>
 		<action name="searchTrade" class="com.changh.sccms.action.trade.SimpleSearchAction">
 			<result name="success" type = "json">
 				<param name="root">tradeList</param>
 			</result>
 		</action>
 	</package>
 </struts>